<Screen3>: #COACH AND SEATING
    name: "screen_three"

    MDBoxLayout:
		orientation: "vertical"
		spacing: 0
		padding: 20
		MDBoxLayout: #layout A
		    orientation: "vertical"
		    size_hint_y: None
            height: 100
		    spacing: 5
		    padding: 5
		    MDCard: #layout A1
		        padding: 5
		        MDIconButton:
                    icon: "chevron-left"
                    pos_hint: {'center_x': 0.2, 'center_y': 0.5}
                    on_release:
                        root.manager.transition.direction = "right"
                        root.manager.current = "screen_two"
                MDLabel:
                    text: "Select Coach and Seat"
                    halign: "center"
                    pos_hint: {'center_x': 0.5, 'center_y': 0.5}
                    padding: 0,5,0,0
                    font_style: "Subtitle1"
            MDBoxLayout: #layout A2
                orientation: "horizontal"
                spacing: 5
                MDCard:
                    padding: 5
                    FitImage:
                        source: "available.png"
                        pos_hint: {'center_x': 0.5, 'center_y': 0.5}
                        size_hint: None, None
                        width: 20
                        height: 20
                    MDLabel:
                        text: "Available"
                        halign: "center"
                        pos_hint: {'center_x': 0.5, 'center_y': 0.5}
                        font_style: "Subtitle2"
                MDCard:
                    padding: 5
                    FitImage:
                        source: "selected.png"
                        pos_hint: {'center_x': 0.5, 'center_y': 0.5}
                        size_hint: None, None
                        width: 20
                        height: 20
                    MDLabel:
                        text: "Selected"
                        halign: "center"
                        pos_hint: {'center_x': 0.5, 'center_y': 0.5}
                        font_style: "Subtitle2"
                MDCard:
                    padding: 5
                    FitImage:
                        source: "unavailable.jpg"
                        pos_hint: {'center_x': 0.5, 'center_y': 0.5}
                        size_hint: None, None
                        width: 20
                        height: 20
                    MDLabel:
                        text: "Unavailable"
                        halign: "center"
                        pos_hint: {'center_x': 0.5, 'center_y': 0.5}
                        font_style: "Subtitle2"

