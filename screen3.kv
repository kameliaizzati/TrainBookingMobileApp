		    spacing: 5
		    padding: 5
		    MDCard: #coach #LayoutB1
		        size_hint_x: None
                width: 95
		        MDBoxLayout:
                    orientation: "vertical"
                    spacing: 5
                    padding: 5
                    CoachButton1:
                        id: tb_a 
                        text: "Coach A"
                        halign: "center"
                        font_size: "12sp"
                        on_press: root.coach_check(root.ids.tb_a.text)
                    CoachButton1:
                        id: tb_b
                        text: "Coach B"
                        halign: "center"
                        font_size: "12sp"
                        on_press: root.coach_check(root.ids.tb_b.text)
                    CoachButton1:
                        id: tb_c
