		                font_style: "Subtitle2"
		                halign: "center"
            MDRectangleFlatButton:
                text: "Next"
                bold: True
                halign: "center"
                font_size: "14sp"
                pos_hint: {'center_x': 0.9, 'center_y': 0.1}
                padding: 5
                on_release:
                    app.pages_coach_seat()
                    root.manager.transition.direction = "left"
                    root.manager.current = "screen_four"

<CoachButton1@ToggleButton>:
