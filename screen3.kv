		MDBoxLayout: #LayoutC
		    orientation: "vertical"
		    spacing: 5
		    padding: 5
		    MDCard:
		        MDGridLayout:
		            cols: 2
		            MDLabel:
		                text: "Coach No"
		                font_style: "Subtitle2"
		                halign: "center"
		            MDLabel:
		                id: selected_coach_3
		                text: "Selected Coach"
		                font_style: "Subtitle2"
		                halign: "center"
		            MDLabel:
		                text: "Seat No "
		                font_style: "Subtitle2"
